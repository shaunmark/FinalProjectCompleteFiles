<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/deleteVehicleText"
        android:layout_width="110dp"
        android:layout_height="match_parent"
        android:layout_gravity="top"
        android:background="@drawable/table_content_cell_bg"
        android:text="Rank"
        android:textColor="#000000"
        android:textSize="18sp" />


    <TextView
        android:id="@+id/searchInDateText"
        android:layout_width="100dp"
        android:layout_height="match_parent"
        android:layout_gravity="top"
        android:background="@drawable/table_content_cell_bg"
        android:text="Year"
        android:textColor="#000000"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/searchInTimeText"
        android:layout_width="100dp"
        android:layout_height="match_parent"
        android:layout_gravity="top"
        android:background="@drawable/table_content_cell_bg"
        android:textColor="#000000"
        android:text="Cost (in millions)"
        android:textSize="18sp" />
    <TextView
        android:id="@+id/searchLogOutDate"
        android:layout_width="102dp"
        android:layout_height="match_parent"
        android:layout_gravity="top"
        android:background="@drawable/table_content_cell_bg"
        android:text="Year"
        android:textColor="#000000"
        android:textSize="18sp" />
    <TextView
        android:id="@+id/searchLogOutTime"
        android:layout_width="102dp"
        android:layout_height="match_parent"
        android:layout_gravity="top"
        android:background="@drawable/table_content_cell_bg"
        android:text="Year"
        android:textColor="#000000"
        android:textSize="18sp" />

</LinearLayout>